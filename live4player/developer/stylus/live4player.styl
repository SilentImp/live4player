.live4player
  width 624px 
  height 351px
  display block
  overflow hidden
  position relative
  font-size 0
  line-height 0
  user-select none
  
  &.live4player-fsmode
    width 100%
    height 100%
    position fixed
    top 0
    bottom 0
    right 0
    left 0
    z-index 32000
    .live4player-navigation-fson
      display none
    .live4player-navigation-fsoff
      display block
      
  &.live4player-playing
    .live4player-navigation-play
      display none
    .live4player-navigation-pause
      display block
      
  &.live4player-muted
    .live4player-navigation-mute
      display none
    .live4player-navigation-unmute
      display block
      
  &.live4player-loading
    .live4player-loading-screen
      opacity 1
      
  &.live4player-static
    .live4player-load
      display block
    .live4player-loading,
    .live4player-navigation
      display none
      
  &-base
    width 100%
    height 100%
    display block
    position absolute
    z-index 1
    
  &-video
    width 100%
    height 100%
    display block
    background-color #000
    z-index 2
    position relative
    pointer-events none
    
  &-watermark
    position absolute
    top 16px
    right 16px
    z-index 3
    width 56px
    height 44px
    background-image url("../svg/watermark.svg") 
    background-repeat no-repeat
    background-position 50% 50%
    background-size 56px auto
    opacity .6 
    pointer-events none
  &-loading-screen
    display block
    opacity 0
    position absolute
    top 0
    bottom 0
    left 0
    right 0
    background-color #000 
    background-color rgba(0,0,0,.5) 
    z-index 4
    transition opacity .25s
    pointer-events none
    &-icon
      margin auto
      font-size 25px
      width 1em
      height 1em
      border-radius 50%
      top 0
      bottom 0
      right 0
      left 0
      position absolute
      text-indent -9999em
      animation live4playerloading 1.1s infinite ease

  &-load
    cursor pointer
    display none
    position absolute
    top 0
    bottom 0
    left 0
    right 0
    background-color rgba(0,0,0,0) 
    background-image url('../svg/play-button.svg') 
    background-repeat no-repeat
    background-position 50% 50%
    background-size 100px 100px
    z-index 6
    opacity 1
    transition opacity .25s
    &:hover
      opacity .8 
      
  &-navigation
    z-index 5
    position absolute
    height 35px
    width 100%
    bottom -35px
    left 0
    right 0
    transition bottom .25s
    background-color #000
    background-color rgba(0,0,0,.40)
    pointer-events none
    user-select none
    &-play
      position absolute
      left 0
      width 40px
      height 35px
      background-image url('../svg/play.svg') 
      background-repeat no-repeat
      background-position 50% 50%
      cursor pointer
      display block
      pointer-events all
      user-select none
    &-pause
      position absolute
      left 0
      width 40px
      height 35px
      background-image url('../svg/pause.svg') 
      background-repeat no-repeat
      background-position 51% 50%
      cursor pointer
      display none
      pointer-events all
      user-select none
    &-unmute
      position absolute
      
      right 44px
      width 40px
      height 35px
      background-image url('../svg/mute.svg') 
      background-repeat no-repeat
      background-position 10px 8px
      cursor pointer
      display none
      pointer-events all
      user-select none
    &-mute
      position absolute
      right 44px
      width 40px
      height 35px
      background-image url('../svg/unmute.svg') 
      background-repeat no-repeat
      background-position 11px 8px
      cursor pointer
      display block
      pointer-events all
      user-select none
    &-fsoff
      position absolute
      right 4px
      width 40px
      height 35px
      background-image url('../svg/fs-off.svg') 
      background-repeat no-repeat
      background-position 10px 8px
      cursor pointer
      display none
      pointer-events all
      user-select none
    &-fson
      position absolute
      right 4px
      width 40px
      height 35px
      background-image url('../svg/fs-on.svg') 
      background-repeat no-repeat
      background-position 12px 8px
      cursor pointer
      display block
      pointer-events all
      user-select none
    &-time
      position absolute
      left 40px
      top 0
      bottom 0
      line-height 35px
      font-size 14px
      color #FFF
      font-family 'Cytia Pro Light' 
      white-space nowrap
      user-select none
      .live4player-navigation-time-current
        display inline-block
        width 40px
        text-align right
        user-select none
      .live4player-navigation-time-duration
        display inline-block
        width 40px
        text-align left
        user-select none
    &-progressbar
      height 7px
      width 100%
      position absolute
      overflow hidden
      bottom 100%
      left 0
      right 0
      transition height .25s
      &-progress
        position absolute
        left 0
        top 0
        bottom 0
        pointer-events none
        background-color rgba(#D0021B ,.6)
        border-right 1px solid #D0021B
        box-shadow inset -1px 0 0 0 rgba(0,0,0,.3)
        box-sizing border-box
        max-width 100%
        width 0%
      &-seekable
        background-color rgba(#D0021B ,.2)
        position absolute
        left 0
        top 0
        bottom 0
        cursor pointer
        max-width 100%
        z-index 2
        pointer-events all
        user-select none
        
      &-heatmap
        display block
        width 100%
        height 100%
        pointer-events none
        
  .live4player:hover &-navigation
    bottom 0
    &-progressbar
      height 14px
    


